		"ts-node": "10.9.2",
		"tslint-microsoft-contrib": "6.2.0",
		"typescript": "5.4.2",
		"typescript-esbuild": "",
		"typescript-tslint-plugin": "0.5.5",
		"vscode": "1.1.37",
